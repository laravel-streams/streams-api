{
    "name": "streams/api",
    "description": "A RESTful API for Laravel Streams.",
    "type": "streams-addon",
    "authors": [
        {
            "name": "Ryan Thompson",
            "email": "ryan@pyrocms.com"
        }
    ],
    "require": {
        "streams/core": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.19",
        "zircote/swagger-php": "3.2.3"
    },
    "autoload": {
        "psr-4": {
            "Streams\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Streams\\Api\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Streams\\Api\\ApiServiceProvider"
            ]
        }
    },
    "license": "MIT"
}
